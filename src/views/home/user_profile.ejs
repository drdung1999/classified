<body>
<div class="header">
	<%- include("./header") %>
	
	<!-- Submit Ad -->
	<div class="submit-ad main-grid-border">
		<div class="container">
			<h2 class="head">Hồ sơ</h2>
			<div class="post-ad-form">
				<div class="personal-details">
				<form action="">
					<label>Họ và Tên <span>*</span></label>
					<input id="input-username" type="text" class="name" placeholder="Chưa cập nhật" value="<%= user_profile.username %>">
					<div class="clearfix"></div>
					<label>Số điện thoại <span>*</span></label>
					<input id="input-phone-number" type="text" class="phone" placeholder="Chưa cập nhật" value="<%= user_profile.phone_number %>">
					<div class="clearfix"></div>
					<label>Địa chỉ<span>*</span></label>
					<input id="input-address" type="text" class="email" placeholder="Chưa cập nhật" value="<%= user_profile.address %>">
					<div class="clearfix"></div>
				<input id="btn-user-update-profile" type="submit" value="Cập nhật">					
				<div class="clearfix"></div>
				</form>
				</div>
			</div>
		</div>	
	</div>
	<!-- // Submit Ad -->
	
